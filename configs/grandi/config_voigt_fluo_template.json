{
    "n_organisms": 4096,
    "n_elites": 128,
    "n_generations": 250,

    "output_folder_name": "results/voigt/fluo/grandi/GGROUPINDEX/CCELLINDEX",  # relative to pypoptim
    # "initial_population_filename": "PATH/backup.pickle",

    # genetic algorithm
    "crossover_rate": 1.0,
    "mutation_rate": 1.0,
    "gamma": 1.0,
    "loss": "V_CaT_shared",
    "columns_control": ["V", "CaT"],
    "columns_model":   ["V", "fluo_mean"],

    # model
    "filename_so": "src/model_ctypes/_grandi/model.so",
    "filename_legend_states": "src/model_ctypes/_grandi/legend_states.csv",
    "filename_legend_constants": "src/model_ctypes/_grandi/legend_constants.csv",

    # needed for ctypes
    "t_run": 9, # or "n_beats": 9
    "t_sampling": 1,
    "tol": 1e-4,
    "stim_period_legend_name": "stim_period",  # STIM_PERIOD for koivumaki


    "experimental_conditions": {
        "common": {
            "params": {
                "amplitude": -60.0,
                "stim_duration": 1,

                "Mg_i": 1.0,  # vanessa
                "Na_o": 140,
                "Ca_o": 2.0,
                "K_o": 4,

                "INa_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "INaB_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "INaCa_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "INaK_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "ICaL_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "ICaB_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "ICaB_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IpCa_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "Ito_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IKr_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IKs_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IK1_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IKp_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IKur_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IClCa_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "IClB_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "koCa": {  # RyR
                    "bounds": [10, 30],
                    "gamma": 0.015,
                    "is_multiplier": false
                },
                "Jserca_scaler": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "fluo_tot": {
                    "bounds": [0.01, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                }
            }
        },

        "CL_VALUE": {"params": {"stim_period": "STIM_PERIOD_VALUE",
                          "Na_i": {"bounds": [4, 16],
                                  "gamma": 0.015},
                          "K_i": {"bounds": [100, 160],
                                 "gamma": 0.015},
                          "Ca_sr": {"bounds": [0.06, 6],
                                   "gamma": 0.015}
                         },
               "filename_phenotype": "data/voigt/GGROUPINDEX/CCELLINDEX/phenotypes/phenotype_CL_PHENOTYPE_raw.csv",
               "filename_state": "data/grandi/fluo/states/state_CL_STATE.txt"
        }
    }
}
