{

    "seed": 42,

    "n_organisms": 4096,
    "n_elites": 128,
    "n_generations": 500,

    "output_folder_name": "results/test",
    # "initial_population_filename": "PATH/backup.pickle",

    # genetic algorithm
    "crossover_rate": 1.0,
    "mutation_rate": 1.0,
    "gamma": 1.0,
    "loss": "V_CaT_shared",
    "ridge_regression": false,
    "columns_control": ["V", "fluo"],
    "columns_model":   ["V", "fluo"],

    # model
    "filename_so": "src/model_ctypes/_koivumaki/koivumaki.so",
    "filename_legend_states": "src/model_ctypes/_koivumaki/legend_states.csv",
    "filename_legend_constants": "src/model_ctypes/_koivumaki/legend_constants.csv",

    # needed for ctypes
    "t_run": 9, # or "n_beats": 9
    "t_sampling": 0.001,
    "tol": 1e-4,
    "stim_period_legend_name": "STIM_PERIOD",

    "experimental_conditions": {
        "common": {
            "params": {

                # "Mg_i": 1.0,  # vanessa
                "Na_o": 140,
                "Ca_o": 2.0,
                "K_o": 4,
                "G_seal": 0,

                "STIM_LEVEL": {
                    "bounds": [0.33, 1.33],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "PNa": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gCaL": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gt": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gKur": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gK1": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gKr": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gKs": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "INaKmax": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "ICaPmax": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gIf": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "kNaCa": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "Jrel_multiplier": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "cpumps": {  # SERCA
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "kSRleak": {
                    "bounds": [0.1, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "fluo_tot": {
                    "bounds": [0.01, 10],
                    "gamma": 0.015,
                    "is_multiplier": true
                },

                "gNab": {
                    "bounds": [0.01, 3],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gCab": {
                    "bounds": [0.01, 3],
                    "gamma": 0.015,
                    "is_multiplier": true
                },
                "gKb": {
                    "bounds": [0.01, 3],
                    "gamma": 0.015,
                    "is_multiplier": true
                }
            }
        },

        "CL_VALUE": {"params": {"STIM_PERIOD": "STIM_PERIOD_VALUE",
                          "Nai": {"bounds": [4, 16],
                                  "gamma": 0.03},
                          "Ki": {"bounds": [100, 180],
                                 "gamma": 0.15},
                          "CaSR": {"bounds": [0.06, 6],
                                   "gamma": 0.015}

                         },
               "filename_phenotype": "data/koivumaki/fluo_10uM_ghk/phenotypes/phenotype_CL_PHENOTYPE.csv",
               "filename_state": "data/koivumaki/fluo_10uM_ghk/states/state_CL_STATE.csv"
        }
    }
